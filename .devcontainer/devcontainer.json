{
	"name": "Codespaces Rust Starter",
	"image": "mcr.microsoft.com/devcontainers/universal:2-linux",
	"extensions": [
		"rust-lang.rust-analyzer",
	],
	"dockerFile": "Dockerfile",
	"settings": {
		"editor.formatOnSave": true,
		"files.exclude": {
			"**/CODE_OF_CONDUCT.md": true,
			"**/LICENSE": true
		}
	}
}